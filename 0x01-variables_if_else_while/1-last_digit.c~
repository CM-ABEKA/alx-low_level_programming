#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main - this is the second project comparing n to 6
 *
 * Description: this takes a value n and prints a string depending on the value.
 * Return: 0 (Success)
 */
int main(void)
{
	int n;
	int last_digit;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	last_digit = n % 10;
	if (n < 6)
	{
		if (n == 0)
		{
			printf("Last digit of %i is %i and is 0", n, last_digit);
		} else
		{
			pritnf("Last digit of %i is %i and is less than 6 and not zero", n, last_digit);
		}
	} else
	{
		printf("Last digit of %i is %i and is greater than 5", n, last_digit);
	return (0);

}
